package socialenterprise.eligrules;

import java.lang.Number;

rule "RegionRule"
	dialect "mvel"
	when
		member : MemberDO( zip not in ( '01234', '12345', '03456', '07896' ) )
	then
		eligrulestatusDO status = new eligrulestatusDO();
		insert( status );
		System.out.println("Region rule violated", member not eligible");
		insert( new eligrulestatusDO() );
end
